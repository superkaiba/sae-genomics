# Environment variables for SAE Genomics

# Experiment tracking
WANDB_API_KEY=your_wandb_api_key_here
WANDB_PROJECT=sae-genomics
WANDB_ENTITY=your_wandb_entity

# Database API keys
# DisGeNET (required for full access)
DISGENET_API_KEY=your_disgenet_api_key

# OMIM (required for API access)
OMIM_API_KEY=your_omim_api_key

# HuggingFace (optional, for model downloads)
HF_TOKEN=your_huggingface_token

# Compute configuration
CUDA_VISIBLE_DEVICES=0
TORCH_HOME=./data/torch_cache

# Logging
LOG_LEVEL=INFO
LOG_FILE=logs/sae_genomics.log

# Data paths (customize as needed)
DATA_ROOT=./data
RESULTS_ROOT=./results
CACHE_ROOT=./data/cache
